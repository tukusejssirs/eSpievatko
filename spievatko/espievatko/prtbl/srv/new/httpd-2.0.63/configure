  ap_last="${ap_cur}"
  ap_cur=`eval "echo ${ap_cur}"`
done
exp_exec_prefix="${ap_cur}"


  APACHE_VAR_SUBST="$APACHE_VAR_SUBST exp_exec_prefix"



